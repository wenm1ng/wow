.footer .github-logo {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
}

.footer .github-uri {
    margin-top: 15rpx;
    font-size: 24rpx;
    color: #999;
}
.footer {
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    position: absolute;
    left: 0;
    bottom: 100rpx;
}
/* 外层组件的宽度可设置成100%或具体数值 */
.adContainer {
    position: fixed;
    bottom: 0;
    width: 100%;
}