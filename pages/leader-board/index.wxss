/*组件背景颜色*/
.zr {

    background: #0396FF;
    width: 100%;
    height:100px; position: relative;
}

.water {
    position: absolute;
    left: 0;
    bottom: -10px;
    height: 30px;
    width: 100%;
    z-index: 1
}

.water-c {
    position: relative
}

.water-1 {
    background: url("data:image/svg+xml;base64,") repeat-x;
    background-size: 600px;
    -webkit-animation: wave-animation-1 3.5s infinite linear;
    animation: wave-animation-1 3.5s infinite linear
}

.water-2 {
    top: 5px;
    background: url("data:image/svg+xml;base64,") repeat-x;
    background-size: 600px;
    -webkit-animation: wave-animation-2 6s infinite linear;
    animation: wave-animation-2 6s infinite linear
}

.water-1,
.water-2 {
    position: absolute;
    width: 100%;
    height: 60px
}

.back-white {
    background: #fff
}

@keyframes wave-animation-1 {
    0% {
        background-position: 0 top
    }
    100% {
        background-position: 600px top
    }
}

@keyframes wave-animation-2 {
    0% {
        background-position: 0 top
    }
    100% {
        background-position: 600px top
    }
}
.user_box {
    display: flex;
    z-index: 10000!important;
}

.userInfo_name {
    flex: 1;
    vertical-align: middle;
    width: 100%;
    margin-left: 5%;
    margin-top: 5%;
}

.userInfo {
    flex: 1;
    width: 100%;
    border-radius: 50%;
    overflow: hidden;
    max-height: 50px;
    max-width: 50px;
    margin-left: 5%;
    margin-top: 5%;
    border: 2px solid #fff;
}

.board-title{
    position: relative;
    left:40%;
    top: 90%;
    font-size: 40rpx;
    z-index:99;
}

.board-caption{
    position: relative;
    left:2%;
    top: 90%;
    font-size: 25rpx;
    z-index:99;
    color:#1f7dd0;
}

.hover-color{
    background-color: #999;
}

.history{
    text-align: center;
}

.card {
    width: 100% !important;
    margin-bottom: 20rpx !important;
    border-radius: 0 !important;
    padding: 20rpx 30rpx !important;
    box-shadow: none !important;
}

.card-img{
    width:0 !important;
    height:0 !important;
}

.margin-lefts{
    margin-left:20rpx
}

.nick-name {
    color: #337559 !important;
    font-weight: bold;
}

.nick-name-anon {
    color: #7a8c9a !important;
    font-weight: bold;
}

.avater-content {
    color: #333;
    text-align: justify;
    font-size: 28rpx;
    margin: 10rpx 0;
}

.content-close {
    display: -webkit-box;
    word-break: break-all;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 4;
    overflow: hidden;
    text-overflow: ellipsis;
    color: #333;
    font-size: 28rpx;
    margin: 10rpx 0;
}

.text-flod {
    color: #0f6945;
    font-weight: bold;
    margin-bottom: 5rpx;
}

.avater-share-container {
    display: flex;
    flex-direction: row;
    height: 60rpx;
    width: 90%;
    margin: 0 auto;
    margin-top: 10rpx;
    align-items: center;
    justify-content: space-between;
}

.avater-share-container .like-container {
    display: flex;
    flex-direction: row;
    align-items: center;
    height: 60rpx;
}

.like-container .number {
    margin-left: 10rpx;
}

.like-container l-icon {
    font-weight: bold;
}

.has-text {
    color: #d81e05;
}

.posters {
    display: flex;
    flex-direction: row;
    margin-top: 10rpx;
    flex-wrap: wrap;
}

.posters image {
    width: 180rpx;
    height: 180rpx;
    margin-right: 10rpx;
    margin-bottom: 10rpx;
}

.posters .column-one {
    width: 360rpx;
}

.posters .column-two {
    width: 270rpx;
    height: 270rpx;
}

.ellipsis {
    margin-top: -38rpx;
}

.label-box {
    display: flex;
    flex-direction: row;
    margin: 25rpx 0 10rpx 0;
}

.tag {
    margin-right: 10rpx;
}

.star-box {
    display: flex;
    flex-direction: row;
    align-items: center;
    background: #fff;
    padding: 20rpx 30rpx;
    border-bottom: 1px #f1f1f1 solid;
}

.star-box .star-content {
    display: flex;
    flex-direction: row;
    align-items: center;
    width: 580rpx;
    flex-wrap: wrap;
}

.star-box .star-icon-box {
    display: flex;
    flex-direction: row;
    align-items: center;
    color: #777;
    font-size: 24rpx;
    margin-left: 20rpx;
}

.star-icon-box .star-icon {
    margin-left: 10rpx;
}

.comment-box {
    width: 100%;
    background: #fff;
    height:100%;
}

.comment-box .comment {
    display: flex;
    flex-direction: row;
    padding: 0 10rpx;
    height:160rpx;
}

.first{
    font-size: 40rpx;
    color: #CF5B42;
}
.second{
    font-size: 35rpx;
    color: #5C96D7;
}
.third{
    font-size: 30rpx;
    color: #E6C63C;
}

.avatar {
    height: 100rpx;
    width: 120rpx;
    border-radius: 50%;
    margin-top: 15rpx;
}

.rank{
    margin-top: 15rpx;
    height: 100rpx;
    width: 100rpx;
}

.comment .comment-right {
    display: flex;
    flex-direction: column;
    margin-left: 30rpx;
    padding: 20rpx 0;
    width: 600rpx;
    border-bottom: 1px #f1f1f1 solid;
}

.comment-top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding-bottom: 8rpx;
}

.comment-top .time {
    color: #666;
    font-size: 24rpx;
}

.comment-content {
    display: flex;
    flex-direction: row;
}

.star-content .avatar {
    margin: 0 10rpx 0 0;
}

.non-border {
    border-bottom: none !important;
}

.more {
    color: #337559;
    font-size: 24rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    /*align-items: center;*/
    height:200rpx;
}

.more-icon {
    display: flex;
    justify-content: center;
    /*align-items: center;*/
    margin-left: 10rpx;
}

.comment-edit {
    border-top: 1px #f1f1f1 solid;
    display: flex;
    flex-direction: row;
    align-items: center;
    position: fixed;
    bottom: 0;
    left: 0;
    width: 100%;
    background: #fff;
    height: 88rpx;
}

.comment-bottom {
    display: flex;
    flex-direction: row;
}

.comment-dibu{
    display: block;
    width:100%;
    float:left;
}

.content {
    width: 80%;
    /*text-align: center;*/
}

.l-img {
    width: 0rpx !important;
    margin-right: 0 !important;
}

.l-card{
    margin-bottom: 20rpx !important;
}

.reply {
    display: flex;
    flex-direction: row;
}

.delete-box {
    margin-left: 20rpx;
}

.delete-text {
    color: #337559;
    font-size: 26rpx;
    font-weight: bold;
    line-height: 40rpx;
    /*width: 60rpx;*/
    /*text-align: right;*/
}

.section{
    height:80rpx;
    line-height: 80rpx;
    background-color: #d4cdcd;
    margin-top:100rpx;
}

.icon{
    display: inline;
    margin-left:10rpx;
}

.loading {
    padding-bottom: 20rpx !important;
    height:50rpx !important;
    display:block;
}